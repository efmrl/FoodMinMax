{
  "name": "foodminmax",
  "version": "1.0.0",
  "description": "small database to track desirability of foods",
  "main": "efmrl2.config.js",
  "scripts": {
    "build:css": "mkdir -p public && sass --load-path=node_modules src/styles.scss public/styles.css --style=compressed",
    "build:favicon": "node build/favicon.cjs",
    "build:md": "node build/credits.cjs",
    "build:copy": "cp src/index.html src/foodOptimizer.js src/favicon.svg public/",
    "build": "npm run build:css && npm run build:favicon && npm run build:md && npm run build:copy",
    "watch:css": "mkdir -p public && sass --load-path=node_modules --watch src/styles.scss:public/styles.css",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/efmrl/FoodMinMax.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/efmrl/FoodMinMax/issues"
  },
  "homepage": "https://github.com/efmrl/FoodMinMax#readme",
  "devDependencies": {
    "@picocss/pico": "^2.1.1",
    "@vitest/coverage-v8": "^4.0.2",
    "happy-dom": "^20.0.8",
    "markdown-it": "^14.1.0",
    "sass": "^1.93.2",
    "sharp": "^0.34.4",
    "vitest": "^4.0.2"
  }
}
